@{
    Layout = "_AdminLayout";
  
}

 <div class="u-s-p-b-60">   
                <div class="section__content">
                    <div class="dash">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-3 col-md-12">

                                  
                                    <div class="dash__box dash__box--bg-white dash__box--shadow u-s-m-b-30" style="text-align:end;">
                                        <div class="dash__pad-1">

                                            <span class="dash__text u-s-m-b-16">سلام ادمین</span>
                                            <ul class="dash__f-list">
                                                <li>

                                                    <a  href="~/admin/ShowProduct" >دیدن محصولات</a></li>
                                                <li>

                                                    <a class="dash-active" href="~/admin/AddProduct">افزودن محصول جدید</a></li>
                                                <li>

                                                    <a  href="~/admin/ShowOrder"> دیدن سفارشات </a></li>
                                                <li>

                                                    <a href="~/admin/medias"> تصاویر </a></li>
                                                <li>

                                                    <a href="~/admin/categorys">دسته بندی ها </a></li>
                                                <li>

                                                    <a href="~/admin/ShowUsers"> مشاهده مشتریان </a></li>
                                             
                                            </ul>
                                        </div>
                                    </div>
                                 </div>

                                  <div class="col-lg-9 col-md-12">
                                    <div class="dash__box dash__box--shadow dash__box--radius dash__box--bg-white">
                                        <div class="dash__pad-2">
                                            <h1 class="dash__h1 u-s-m-b-14" style="text-align:right;">افزودن کالای جدید</h1>

                                            @*<span class="dash__text u-s-m-b-30">We need an address where we could deliver products.</span>*@
                                            <form class="dash-address-manipulation">
                                                <div class="gl-inline" dir="rtl">
                                                    <div class="u-s-m-b-30"  dir="rtl">

                                                        <label class="gl-label" for="address-fname" style="text-align:right">نام کالا *</label>

                                                        <input class="input-text input-text--primary-style" type="text" id="address-fname" placeholder="Name"></div>
                                                    <div class="u-s-m-b-30"  dir="rtl">

                                                        <label class="gl-label" for="address-lname" style="text-align:right">توضیحات *</label>

                                                        <input class="input-text input-text--primary-style" type="text" id="address-lname" placeholder="Description"></div>
                                                </div>
                                                <div class="gl-inline">
                                                    <div class="u-s-m-b-30" dir="rtl">

                                                        <label class="gl-label" for="address-phone" style="text-align:right">تاریخ تولید *</label>

                                                        <input class="input-text input-text--primary-style" type="text" id="address-phone" placeholder="yyyy/mm/dd"></div>
                                                    <div class="u-s-m-b-30" dir="rtl">

                                                        <label class="gl-label" for="address-street" style="text-align:right"> قیمت  *</label>

                                                        <input class="input-text input-text--primary-style" type="text" id="address-street" placeholder="Price"></div>
                                                </div>
                                               
                                               @* <div class="gl-inline">
                                                    <div class="u-s-m-b-30">

                                                        <label class="gl-label" for="address-city">TOWN/CITY *</label>

                                                        <input class="input-text input-text--primary-style" type="text" id="address-city"></div>
                                                    <div class="u-s-m-b-30">

                                                        <label class="gl-label" for="address-street">ZIP/POSTAL CODE *</label>

                                                        <input class="input-text input-text--primary-style" type="text" id="address-postal" placeholder="Zip/Postal Code"></div>
                                                </div>*@

                                                <button class="btn btn--e-brand-b-2" type="submit">ذخیره</button>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                       </div>

                    </div>
                </div>
               
            </div>





      </div>
            